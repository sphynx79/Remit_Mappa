    "scripts": {
        "start": "set next=false && nodemon --watch  webpack.dev.js --watch webpack.common.js --exec \"webpack-dev-server --colors --progress --config webpack.dev.js\"",
        "start_ssl": "set next=false && set ssl=true && nodemon --watch  webpack.dev.js --watch webpack.common.js --exec \"webpack-dev-server --colors --config webpack.dev.js\"",
        "build": "set next=false && webpack --config webpack.prod.js",
        "start:next": "set next=true && nodemon --watch  webpack.dev.js --watch webpack.common.js --exec \"webpack-dev-server --colors --config webpack.dev.js\"",
        "build:next": "set next=true && webpack --config webpack.prod.js",
        "watch": "webpack --watch --progress --colors --display-error-details --config webpack.watch.js"
    },
